entity Image {
	filePath String,
	type SctType,
	MandantId String,
	KundenId String,
    ImageName String,
    validFrom LocalDate,
    validTo LocalDate
}

enum SctType {
    EPSFEIN, EPSGROB, PDFFEIN, PDFGROB, PREVIEWGROB
}

dto * with mapstruct

// Set service options to all except few
service all with serviceImpl
